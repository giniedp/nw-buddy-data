							<SetMannequinTag ResetWhenReinstalled="true">
								<Default TagName="ReloadRequired" OnEnter="Unset" OnExit="NoEffect" DelayTimer="0.0"/>
							</SetMannequinTag>
						</Group>
					</ThenActivity>
				</IfThenElse>
			</Activities>
