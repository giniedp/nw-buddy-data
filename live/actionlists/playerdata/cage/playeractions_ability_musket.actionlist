						<SetMannequinTag ResetWhenReinstalled="false">
							<Default TagName="AimDownSights" OnEnter="Unset" OnExit="Unset" DelayTimer="0"/>
						</SetMannequinTag>
					</ElseActivity>
				</IfThenElse>
				<AnimationDrivenMotion>
