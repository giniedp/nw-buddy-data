	<Conditions>
		<IfMannequinTag Name="ChainAttack" TagName="ChainAttack"/>
		<IfMannequinTag Name="StopChainAttack" TagName="StopChainAttack"/>
		<IfMannequinMarker Name="StopFacingTarget" MarkerName="StopFacingTarget"/>
		<AIDistanceToTarget Name="SwoopSlash_Short" Comparison="LessThan" DistanceToTarget="18.0"/>
		<AIDistanceToTarget Name="SwoopSlash_Long" Comparison="GreaterThanOrEqual" DistanceToTarget="17.0"/>
		<And Name="Swoop_Transition">
			<Or>
				<AIAngleToTarget Comparison="GreaterThanOrEqual" Angle="90.0" AbsoluteValue="true"/>
				<AIDistanceToTarget Comparison="LessThanOrEqual" DistanceToTarget="1.0"/>
			</Or>
			<Not>
				<IfMannequinTag TagName="StopChainAttack"/>
			</Not>
		</And>
		<And Name="Swoop_Always_Transition">
			<True/>
			<Not>
				<IfMannequinTag TagName="StopChainAttack"/>
			</Not>
		</And>
		<And Name="Swoop_End">
			<Or>
				<AIAngleToTarget Comparison="GreaterThanOrEqual" Angle="90.0" AbsoluteValue="true"/>
				<AIDistanceToTarget Comparison="LessThanOrEqual" DistanceToTarget="1.0"/>
			</Or>
			<IfMannequinTag TagName="StopChainAttack"/>
		</And>
		<And Name="Swoop_Always_End">
			<True/>
			<IfMannequinTag TagName="StopChainAttack"/>
		</And>
	</Conditions>
</ActionList>

