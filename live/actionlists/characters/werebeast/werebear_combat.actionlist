				</AnimationDrivenMotion>
				<IfThenElse ResetWhenReinstalled="true" Comment="">
					<IfCondition>
						<IfMannequinMarker MarkerName="Activate_Enrage"/>
					</IfCondition>
					<ThenActivity>
						<Group>
							<ResetStamina ResetWhenReinstalled="false" IgnoreDataChanges="false">
								<Default OnEnter="Reset" OnExit="NoEffect" Comment="Reset stamina at the end of the action"/>
							</ResetStamina>
							<ModifyStatusEffects ResetWhenReinstalled="false">
								<Default OnlyRemoveOwnedStatusEffects="false" OnlyRemoveOneStack="false" RemoveNothingOnMigrate="false">
									<EffectsToApplyOnEnter>
										<EffectId EffectId="AI_Enable_Stagger"/>
										<EffectId EffectId="AI_Werebear_Enraged"/>
									</EffectsToApplyOnEnter>
								</Default>
							</ModifyStatusEffects>
						</Group>
					</ThenActivity>
				</IfThenElse>
			</Activities>
		</Action>
		<Action Name="Enrage" FragmentName="">
			<ActivationCondition>
				<And>
					<Not>
						<IfMannequinTag TagName="Stunned"/>
					</Not>
					<IfMannequinTag TagName="Alerted"/>
					<IfHasAttackTarget/>
					<Not>
						<IfDead/>
					</Not>
				</And>
			</ActivationCondition>
			<DeactivationCondition/>
			<Activities>
				<ModifyStatusEffects>
					<Default Comment="Controller adds stacks of enrage every X seconds">
						<EffectsToApplyOnEnter>
							<EffectId EffectId="AI_Werebear_Enrage_Controller"/>
						</EffectsToApplyOnEnter>
						<EffectsToRemoveOnExit>
							<EffectId EffectId="AI_Werebear_Enrage_Controller"/>
							<EffectId EffectId="AI_Werebear_Enraged"/>
							<EffectId EffectId="AI_Werebear_Enrage_Buff"/>
							<EffectId EffectId="AI_Enable_Stagger"/>
						</EffectsToRemoveOnExit>
					</Default>
				</ModifyStatusEffects>
			</Activities>
		</Action>
	</Actions>
	<Conditions>
