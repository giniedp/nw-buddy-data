		<Action Name="Attack_Enrage_Activate" FragmentName="Attack_Enrage_Activate" Predictive="true">
			<ActivationCondition>
				<IfBehaviorTreeTask ActionName="Attack_Enrage_Activate_CAGE_Task"/>
			</ActivationCondition>
			<DeactivationCondition>
