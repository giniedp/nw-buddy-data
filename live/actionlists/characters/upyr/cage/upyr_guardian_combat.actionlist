				<IfThenElse ResetWhenReinstalled="true" Comment="Timer for activating Thrall Shield">
					<IfCondition>
						<IfMannequinMarker MarkerName="Activate_Thrall_Shield"/>
					</IfCondition>
					<ThenActivity>
						<Group>
							<ResetStamina ResetWhenReinstalled="true">
								<Default OnEnter="Reset" OnExit="NoEffect"/>
							</ResetStamina>
							<ModifyStatusEffects>
								<Default>
									<EffectsToApplyOnEnter>
										<EffectId EffectId="AI_Upyr_Guardian_ThrallShield_Active"/>
										<EffectId EffectId="AI_Upyr_Guardian_ThrallShield_Fortify"/>
									</EffectsToApplyOnEnter>
								</Default>
							</ModifyStatusEffects>
							<SetMannequinTag ResetWhenReinstalled="true">
								<Default TagName="ThrallShield2" OnEnter="Set" OnExit="NoEffect" DelayTimer="0.0"/>
							</SetMannequinTag>
							<SetMannequinTag ResetWhenReinstalled="true">
								<Default TagName="ThrallShield_Active" OnEnter="Set" OnExit="NoEffect" DelayTimer="0.0"/>
							</SetMannequinTag>
							<TriggerEntity ResetWhenReinstalled="true">
								<Default TriggerName="ThrallShield_Activate"/>
							</TriggerEntity>
						</Group>
					</ThenActivity>
				</IfThenElse>
				<SetAudioSwitchState ResetWhenReinstalled="true" RunOnRemoteClients="true">
					<Default SwitchName="NPC_Enraged" StateName="On" SetOnStop="false"/>
				</SetAudioSwitchState>
			</Activities>
		</Action>
