		<Action Name="Attack_Whirlwind" FragmentName="Attack_Whirlwind">
			<ActivationCondition>
				<IfBehaviorTreeTask ActionName="Attack_Whirlwind_CAGE_Task"/>
			</ActivationCondition>
			<DeactivationCondition>
				<IfFragmentDone UseHostActionFragment="true" FragmentName="" ExitOffsetSeconds="0.0"/>
			</DeactivationCondition>
			<Activities>
				<BehaviorTreeTask>
					<Default InvalidatePlanOnExit="false" InvalidatePlanOnReinstall="false" CompletionFragmentOffset="0.2"/>
				</BehaviorTreeTask>
				<AnimationDrivenMotion>
					<Default MotionMultiplier="1.00" RotationMultiplier="0.00" Decel="20.0,20.0,20.0" DisableGravity="false" InheritVelocity="false"/>
				</AnimationDrivenMotion>
				<ModifyStatusEffects>
					<Default OnlyRemoveOwnedStatusEffects="false" OnlyRemoveOneStack="true">
						<EffectsToRemoveOnExit>
							<EffectId EffectId="AI_Upyr_Guardian_ThrallShield_Fortify"/>
							<EffectId EffectId="AI_Upyr_Guardian_ThrallShield_Elite_Fortify"/>
						</EffectsToRemoveOnExit>
					</Default>
				</ModifyStatusEffects>
				<IfThenElse ResetWhenReinstalled="true" Comment="Decrement Thrall Shield On Use of Empowered Whirlwind">
					<IfCondition>
						<IfMannequinTag TagName="ThrallShield_Active"/>
					</IfCondition>
					<ThenActivity>
						<SetMannequinTag ResetWhenReinstalled="true">
							<Conditional Name="ThrallShield6" TagName="ThrallShield5" OnEnter="NoAction" OnExit="Set"/>
							<Conditional Name="ThrallShield5" TagName="ThrallShield4" OnEnter="NoAction" OnExit="Set"/>
							<Conditional Name="ThrallShield4" TagName="ThrallShield3" OnEnter="NoAction" OnExit="Set"/>
							<Conditional Name="ThrallShield3" TagName="ThrallShield2" OnEnter="NoAction" OnExit="Set"/>
							<Conditional Name="ThrallShield2" TagName="ThrallShield1" OnEnter="NoAction" OnExit="Set"/>
							<Conditional Name="ThrallShield1" TagName="ThrallShield1" OnEnter="NoAction" OnExit="Unset"/>
						</SetMannequinTag>
					</ThenActivity>
				</IfThenElse>
			</Activities>
		</Action>
