		<Action Name="Set_Phase_2" FragmentName="">
			<ActivationCondition>
				<And>
					<IfHealthPercentage Comparison="LessThanOrEqual" Percentage="0.75"/>
					<IfMannequinTag TagName="Boss"/>
					<Not>
						<IfMannequinTag TagName="Phase2"/>
					</Not>
				</And>
			</ActivationCondition>
			<DeactivationCondition>
				<IfMannequinTag TagName="Phase2"/>
			</DeactivationCondition>
			<Activities>
				<SetMannequinTag ResetWhenReinstalled="false">
					<Default TagName="Phase2" OnEnter="Set" OnExit="NoEffect" DelayTimer="0.0"/>
				</SetMannequinTag>
			</Activities>
		</Action>
		<Action Name="Unset_Phase_2" FragmentName="">
			<ActivationCondition>
				<And>
					<IfMannequinTag TagName="Boss"/>
					<IfMannequinTag TagName="Phase2"/>
					<IfHasStatusEffect StatusEffect="ResetAI"/>
				</And>
			</ActivationCondition>
			<DeactivationCondition>
				<Not>
					<IfMannequinTag TagName="Phase2"/>
				</Not>
			</DeactivationCondition>
			<Activities>
				<SetMannequinTag ResetWhenReinstalled="false">
					<Default TagName="Phase2" OnEnter="Unset" OnExit="NoEffect" DelayTimer="0.0"/>
				</SetMannequinTag>
