		<Action Name="Kneeling_Start" FragmentName="Kneeling_Start">
			<ActivationCondition>
				<And>
					<Not>
						<IfFragmentPlaying UseHostActionFragment="true" FragmentName="Kneeling_Start"/>
					</Not>
					<IfHealthPercentage Comparison="LessThanOrEqual" Percentage="0.5"/>
					<IfHealthPercentage Comparison="GreaterThan" Percentage="0"/>
					<Not>
						<BlackboardCondition LeftOperand="%Phase2%" Operator="GreaterThanOrEqual" RightOperand="true"/>
					</Not>
					<Not>
						<IfFragmentPlaying UseHostActionFragment="true" FragmentName="r_DeathsDoor"/>
					</Not>
					<Not>
						<IfDead ServerOnly="true"/>
					</Not>
					<Not>
						<IfMannequinTag TagName="Shadow"/>
					</Not>
					<IfHasAttackTarget/>
				</And>
			</ActivationCondition>
			<DeactivationCondition>
				<IfFragmentDone UseHostActionFragment="true" FragmentName="" ExitOffsetSeconds="0.0"/>
			</DeactivationCondition>
			<Activities>
