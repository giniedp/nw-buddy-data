						<SetMannequinTag ResetWhenReinstalled="true">
							<Default TagName="Kneeling" OnEnter="Set" OnExit="Set" DelayTimer="0.0"/>
						</SetMannequinTag>
					</ThenActivity>
				</IfThenElse>
