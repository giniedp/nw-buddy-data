<ActionList>
	<Actions>
		<Action Name="RemoveProxyOnDeath">
			<ActivationCondition>
				<And>
					<IfHasStatusEffect StatusEffect="AI_PalatineArcher_Death" ComparisonType="Equal" StackSize="1"/>
					<IfHasStatusEffect StatusEffect="AI_PalatineGuards_NearbyCheck" ComparisonType="GreaterThan" StackSize="0"/>
				</And>
			</ActivationCondition>
			<DeactivationCondition>
				<IfHasStatusEffect StatusEffect="AI_PalatineArcher_Death" ComparisonType="Equal" StackSize="0"/>
			</DeactivationCondition>
			<Activities>
				<ModifyStatusEffects>
					<Default OnlyRemoveOwnedStatusEffects="false" OnlyRemoveOneStack="false" RemoveNothingOnMigrate="false">
						<EffectsToRemoveOnEnter>
							<EffectId EffectId="AI_PalatineGuards_NearbyCheck"/>
						</EffectsToRemoveOnEnter>
					</Default>
				</ModifyStatusEffects>
			</Activities>
		</Action>
		<Action Name="RemovePreProxyOnDeath">
			<ActivationCondition>
				<And>
					<IfHasStatusEffect StatusEffect="AI_PalatineArcher_Death" ComparisonType="Equal" StackSize="1"/>
					<IfHasStatusEffect StatusEffect="AI_PalatineGuards_PreProxy" ComparisonType="GreaterThan" StackSize="0"/>
				</And>
			</ActivationCondition>
			<DeactivationCondition>
				<IfHasStatusEffect StatusEffect="AI_PalatineArcher_Death" ComparisonType="Equal" StackSize="0"/>
			</DeactivationCondition>
			<Activities>
				<ModifyStatusEffects>
					<Default OnlyRemoveOwnedStatusEffects="false" OnlyRemoveOneStack="false" RemoveNothingOnMigrate="false">
						<EffectsToRemoveOnEnter>
							<EffectId EffectId="AI_PalatineGuards_PreProxy"/>
						</EffectsToRemoveOnEnter>
					</Default>
				</ModifyStatusEffects>
			</Activities>
		</Action>
		<Action Name="PreProxyON">
			<ActivationCondition>
				<And>
					<IfHasStatusEffect StatusEffect="AI_PalatineGuards_PreProxy" ComparisonType="GreatherThanOrEqual" StackSize="2"/>
					<IfHasStatusEffect StatusEffect="AI_PalatineGuards_NearbyCheck" ComparisonType="LessThanOrEqual" StackSize="1"/>
				</And>
			</ActivationCondition>
			<Activities>
				<TriggerEntity>
					<Default TriggerName="PreProxyON"/>
				</TriggerEntity>
			</Activities>
		</Action>
		<Action Name="PreProxyOFF">
			<ActivationCondition>
				<IfHasStatusEffect StatusEffect="AI_PalatineGuards_PreProxy" ComparisonType="Equal" StackSize="1"/>
			</ActivationCondition>
			<Activities>
				<TriggerEntity>
					<Default TriggerName="PreProxyOFF"/>
				</TriggerEntity>
			</Activities>
		</Action>
		<Action Name="ProxyON">
			<ActivationCondition>
				<IfHasStatusEffect StatusEffect="AI_PalatineGuards_NearbyCheck" ComparisonType="GreatherThanOrEqual" StackSize="2"/>
			</ActivationCondition>
			<Activities>
				<TriggerEntity>
					<Default TriggerName="ProxyON"/>
				</TriggerEntity>
				<TriggerEntity>
					<Default TriggerName="OvercoatON"/>
				</TriggerEntity>
				<TriggerEntity>
					<Default TriggerName="PreProxyOFF"/>
				</TriggerEntity>
			</Activities>
		</Action>
		<Action Name="ProxyOFF">
			<ActivationCondition>
				<IfHasStatusEffect StatusEffect="AI_PalatineGuards_NearbyCheck" ComparisonType="Equal" StackSize="1"/>
			</ActivationCondition>
			<Activities>
				<TriggerEntity>
					<Default TriggerName="ProxyOFF"/>
				</TriggerEntity>
				<TriggerEntity>
					<Default TriggerName="OvercoatOFF"/>
				</TriggerEntity>
			</Activities>
		</Action>
		<Action Name="Nearby1">
			<ActivationCondition>
				<IfHasStatusEffect StatusEffect="AI_PalatineGuards_NearbyCheck" ComparisonType="Equal" StackSize="1"/>
			</ActivationCondition>
			<DeactivationCondition>
				<Not>
					<IfHasStatusEffect StatusEffect="AI_PalatineGuards_NearbyCheck" ComparisonType="Equal" StackSize="1"/>
				</Not>
			</DeactivationCondition>
			<Activities>
				<ModifyStatusEffects>
					<Default OnlyRemoveOwnedStatusEffects="false" OnlyRemoveOneStack="false" RemoveNothingOnMigrate="false">
						<EffectsToRemoveOnEnter>
							<EffectId EffectId="AI_PalatineGuards_SmallSquad"/>
							<EffectId EffectId="AI_PalatineGuards_Fortify1"/>
						</EffectsToRemoveOnEnter>
					</Default>
				</ModifyStatusEffects>
			</Activities>
		</Action>
		<Action Name="Nearby2">
			<ActivationCondition>
				<IfHasStatusEffect StatusEffect="AI_PalatineGuards_NearbyCheck" ComparisonType="Equal" StackSize="2"/>
			</ActivationCondition>
			<DeactivationCondition>
				<Not>
					<IfHasStatusEffect StatusEffect="AI_PalatineGuards_NearbyCheck" ComparisonType="Equal" StackSize="2"/>
				</Not>
			</DeactivationCondition>
			<Activities>
				<ModifyStatusEffects>
					<Default OnlyRemoveOwnedStatusEffects="false" OnlyRemoveOneStack="false" RemoveNothingOnMigrate="false">
						<EffectsToApplyOnEnter>
							<EffectId EffectId="AI_PalatineGuards_SmallSquad"/>
							<EffectId EffectId="AI_PalatineGuards_Fortify1"/>
						</EffectsToApplyOnEnter>
						<EffectsToRemoveOnEnter>
							<EffectId EffectId="AI_PalatineGuards_Fortify2"/>
						</EffectsToRemoveOnEnter>
						<EffectsToRemoveOnExit>
							<EffectId EffectId="AI_PalatineGuards_Fortify1"/>
						</EffectsToRemoveOnExit>
					</Default>
				</ModifyStatusEffects>
			</Activities>
		</Action>
		<Action Name="Nearby3">
			<ActivationCondition>
				<IfHasStatusEffect StatusEffect="AI_PalatineGuards_NearbyCheck" ComparisonType="Equal" StackSize="3"/>
			</ActivationCondition>
			<DeactivationCondition>
				<Not>
					<IfHasStatusEffect StatusEffect="AI_PalatineGuards_NearbyCheck" ComparisonType="Equal" StackSize="3"/>
				</Not>
			</DeactivationCondition>
			<Activities>
				<ModifyStatusEffects>
					<Default OnlyRemoveOwnedStatusEffects="false" OnlyRemoveOneStack="false" RemoveNothingOnMigrate="false">
						<EffectsToApplyOnEnter>
							<EffectId EffectId="AI_PalatineGuards_SmallSquad"/>
							<EffectId EffectId="AI_PalatineGuards_Fortify2"/>
						</EffectsToApplyOnEnter>
						<EffectsToRemoveOnEnter>
							<EffectId EffectId="AI_PalatineGuards_Fortify1"/>
							<EffectId EffectId="AI_PalatineGuards_Fortify3"/>
							<EffectId EffectId="AI_PalatineGuards_FullSquad"/>
						</EffectsToRemoveOnEnter>
						<EffectsToRemoveOnExit>
							<EffectId EffectId="AI_PalatineGuards_Fortify2"/>
						</EffectsToRemoveOnExit>
					</Default>
				</ModifyStatusEffects>
			</Activities>
		</Action>
		<Action Name="Nearby4">
			<ActivationCondition>
				<IfHasStatusEffect StatusEffect="AI_PalatineGuards_NearbyCheck" ComparisonType="Equal" StackSize="4"/>
			</ActivationCondition>
			<DeactivationCondition>
				<Not>
					<IfHasStatusEffect StatusEffect="AI_PalatineGuards_NearbyCheck" ComparisonType="Equal" StackSize="4"/>
				</Not>
			</DeactivationCondition>
			<Activities>
				<ModifyStatusEffects>
					<Default OnlyRemoveOwnedStatusEffects="false" OnlyRemoveOneStack="false" RemoveNothingOnMigrate="false">
						<EffectsToApplyOnEnter>
							<EffectId EffectId="AI_PalatineGuards_FullSquad"/>
							<EffectId EffectId="AI_PalatineGuards_Fortify3"/>
						</EffectsToApplyOnEnter>
						<EffectsToRemoveOnEnter>
							<EffectId EffectId="AI_PalatineGuards_Fortify2"/>
							<EffectId EffectId="AI_PalatineGuards_SmallSquad"/>
						</EffectsToRemoveOnEnter>
						<EffectsToRemoveOnExit>
							<EffectId EffectId="AI_PalatineGuards_FullSquad"/>
							<EffectId EffectId="AI_PalatineGuards_Fortify3"/>
						</EffectsToRemoveOnExit>
					</Default>
				</ModifyStatusEffects>
			</Activities>
		</Action>
		<Action Name="TurnOffStasisIfAlive">
			<ActivationCondition>
				<And>
					<IfFragmentDone UseHostActionFragment="false" FragmentName="Stasis_End" ExitOffsetSeconds="0.0"/>
					<IfMannequinTag TagName="Stasis"/>
				</And>
			</ActivationCondition>
			<DeactivationCondition>
				<Or>
					<IfFragmentPlaying UseHostActionFragment="true" FragmentName="Stasis_End"/>
					<IfFragmentPlaying UseHostActionFragment="true" FragmentName="Stasis_Start"/>
					<Not>
						<IfMannequinTag TagName="Stasis"/>
					</Not>
				</Or>
			</DeactivationCondition>
			<Activities>
				<SetMannequinTag ResetWhenReinstalled="true">
					<Default TagName="Stasis" OnEnter="Unset" DelayTimer="0.0"/>
				</SetMannequinTag>
			</Activities>
		</Action>
		<Action Name="Move_To_Middle" FragmentName="Move_To_Middle">
			<ActivationCondition>
				<IfBehaviorTreeTask ActionName="Move_To_Middle_CAGE_Task"/>
			</ActivationCondition>
			<DeactivationCondition>
