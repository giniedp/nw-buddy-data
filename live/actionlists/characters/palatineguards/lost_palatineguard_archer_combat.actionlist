				</IfThenElse>
				<IfThenElse ResetWhenReinstalled="true" Comment="">
					<IfCondition>
						<Not>
							<IfHasStatusEffect Name="CheckChampion" StatusEffect="AI_Palatine_Champion"/>
						</Not>
					</IfCondition>
					<ThenActivity>
						<ModifyStatusEffects>
							<Default OnlyRemoveOwnedStatusEffects="false" OnlyRemoveOneStack="false" RemoveNothingOnMigrate="false">
								<EffectsToApplyOnEnter>
									<EffectId EffectId="AI_PalatineGuard_StasisState"/>
								</EffectsToApplyOnEnter>
							</Default>
						</ModifyStatusEffects>
					</ThenActivity>
				</IfThenElse>
				<ReviveFromDeathsDoor>
					<Default TriggerOption="OnEnter" DelayTime="0" HealthMultiplier="1"/>
