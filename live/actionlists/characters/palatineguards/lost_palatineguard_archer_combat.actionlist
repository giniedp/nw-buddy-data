		<Action Name="r_Sacrifice_Prep" ServerStarted="true">
			<ActivationCondition>
				<And>
					<IfHasStatusEffect StatusEffect="AI_IoN_EncManagement_SacrificeOrder"/>
					<Not>
						<IfDeathsDoor ServerOnly="true"/>
					</Not>
					<Not>
						<IfDead ServerOnly="true"/>
					</Not>
				</And>
			</ActivationCondition>
			<DeactivationCondition>
				<IfHasStatusEffect StatusEffect="AI_PalatineGuard_Sacrifice_AbyssCloak"/>
			</DeactivationCondition>
			<Activities>
				<BehaviorTreeTask>
					<Default InvalidatePlanOnExit="false" InvalidatePlanOnReinstall="false" CompletionFragmentOffset="0.4"/>
				</BehaviorTreeTask>
				<TriggerEntity>
					<Default TriggerName="Trigger_SacrificeChanneling"/>
				</TriggerEntity>
				<ModifyStatusEffects>
					<Default OnlyRemoveOwnedStatusEffects="false" OnlyRemoveOneStack="false" RemoveNothingOnMigrate="false">
						<EffectsToApplyOnEnter>
							<EffectId EffectId="AI_PalatineGuard_Sacrifice_AbyssCloak"/>
						</EffectsToApplyOnEnter>
						<EffectsToApplyOnExit>
							<EffectId EffectId="AI_PalatineGuard_Sacrifice_AbyssCloak_BurstHealing"/>
						</EffectsToApplyOnExit>
						<EffectsToRemoveOnEnter>
							<EffectId EffectId="AI_IoN_EncManagement_SacrificeOrder"/>
						</EffectsToRemoveOnEnter>
					</Default>
				</ModifyStatusEffects>
				<TriggerEntity>
					<Default TriggerName="Trigger_SacrificeChanneling"/>
				</TriggerEntity>
				<SetMannequinTag ResetWhenReinstalled="true">
					<Default TagName="Sacrificing" OnEnter="Set" DelayTimer="0.0"/>
				</SetMannequinTag>
			</Activities>
		</Action>
		<Action Name="r_Sacrifice_End" FragmentName="r_Sacrifice_End">
			<ActivationCondition>
				<And>
					<IfHasStatusEffect StatusEffect="AI_PalatineGuard_Sacrifice_SacrificeFinale"/>
					<Not>
						<IfFragmentPlaying UseHostActionFragment="true" FragmentName="r_Sacrifice_End" FragmentTime="-1.0f" FragmentTimeComparisonType="GreaterThanOrEqual"/>
					</Not>
				</And>
			</ActivationCondition>
			<DeactivationCondition>
				<IfFragmentDone UseHostActionFragment="true" FragmentName="" ExitOffsetSeconds="0.0"/>
			</DeactivationCondition>
			<Activities>
				<SetReactionFragment>
					<Default ReactionTablePath="SharedAssets/springboardentitites/Characters/BaseAI/CAGE/BaseAI_EmptyReactionTableHarness.actionlist" IsFullBody="true" UnsetReactionOffset="0.0"/>
				</SetReactionFragment>
				<SetMannequinTag ResetWhenReinstalled="true">
					<Default TagName="EndingSacrifice" OnEnter="Set" DelayTimer="0.0"/>
				</SetMannequinTag>
				<TriggerEntity>
					<Default TriggerName="HideNameplate"/>
				</TriggerEntity>
				<ModifyStatusEffects>
					<Default OnlyRemoveOwnedStatusEffects="false" OnlyRemoveOneStack="false" RemoveNothingOnMigrate="false">
						<EffectsToRemoveOnEnter>
							<EffectId EffectId="AI_PalatineGuard_Sacrifice_SacrificeFinale"/>
						</EffectsToRemoveOnEnter>
						<EffectsToApplyOnExit>
							<EffectId EffectId="AI_PalatineGuards_SelfDestruct"/>
						</EffectsToApplyOnExit>
					</Default>
				</ModifyStatusEffects>
			</Activities>
		</Action>
		<Action Name="r_DeathsDoor" FragmentName="r_DeathsDoor">
			<ActivationCondition>
				<And>
					<IfDeathsDoor ServerOnly="true"/>
					<Not>
						<IfFragmentPlaying UseHostActionFragment="true" FragmentName="r_DeathsDoor"/>
					</Not>
					<Not>
						<IfDead ServerOnly="true"/>
					</Not>
					<IfHasAttackTarget/>
				</And>
			</ActivationCondition>
			<DeactivationCondition>
				<IfFragmentDone UseHostActionFragment="true" FragmentName="" ExitOffsetSeconds="0.2"/>
			</DeactivationCondition>
			<Activities>
				<SetReactionFragment>
					<Default ReactionTablePath="SharedAssets/springboardentitites/Characters/BaseAI/CAGE/BaseAI_EmptyReactionTableHarness.actionlist" IsFullBody="true"/>
				</SetReactionFragment>
				<BehaviorTreeTask>
					<Default InvalidatePlanOnExit="false" InvalidatePlanOnReinstall="false" CompletionFragmentOffset="0.0"/>
				</BehaviorTreeTask>
				<TriggerEntity>
					<Default TriggerName="Trigger_SacrificeChannelingStop"/>
				</TriggerEntity>
				<TriggerEntity>
					<Default TriggerName="ProximityOff"/>
				</TriggerEntity>
				<TriggerEntity>
					<Default TriggerName="ProxyOFF"/>
				</TriggerEntity>
				<TriggerEntity>
					<Default TriggerName="PreProxyOFF"/>
				</TriggerEntity>
				<TriggerEntity>
					<Default TriggerName="SignatureGatingOn"/>
				</TriggerEntity>
				<TriggerEntity>
					<Default TriggerName="Trigger_SacrificeChannelingStop"/>
				</TriggerEntity>
				<TriggerEntity>
					<Default TriggerName="DeathsDoorSignal_Activate"/>
				</TriggerEntity>
				<AnimationDrivenMotion>
					<Default MotionMultiplier="1.00" RotationMultiplier="0.00" Decel="20.0,20.0,20.0" DisableGravity="false" InheritVelocity="false"/>
				</AnimationDrivenMotion>
				<IfThenElse ResetWhenReinstalled="true" Comment="">
					<IfCondition>
						<IfMannequinMarker MarkerName="HideNameplate"/>
					</IfCondition>
					<ThenActivity>
						<TriggerEntity>
							<Default TriggerName="HideNameplate"/>
						</TriggerEntity>
					</ThenActivity>
				</IfThenElse>
				<IfThenElse ResetWhenReinstalled="true" Comment="">
					<IfCondition>
						<IfHasStatusEffect StatusEffect="AI_IoN_EncManagement_DahutDeathSignal"/>
					</IfCondition>
					<ThenActivity>
						<TriggerEntity>
							<Default TriggerName="Trigger_TrueDeath"/>
						</TriggerEntity>
					</ThenActivity>
				</IfThenElse>
				<ModifyStatusEffects>
					<Default OnlyRemoveOwnedStatusEffects="false" OnlyRemoveOneStack="false" RemoveNothingOnMigrate="false">
						<EffectsToApplyOnEnter>
							<EffectId EffectId="AI_PalatineShield_Unflinching_Immunity"/>
						</EffectsToApplyOnEnter>
						<EffectsToRemoveOnExit>
							<EffectId EffectId="AI_PalatineShield_Unflinching_Immunity"/>
						</EffectsToRemoveOnExit>
					</Default>
				</ModifyStatusEffects>
				<SetMannequinTag ResetWhenReinstalled="true">
					<Default TagName="DeathsDoor" OnEnter="Set" DelayTimer="0.0"/>
				</SetMannequinTag>
				<SetMannequinTag ResetWhenReinstalled="true">
					<Default TagName="Sacrificing" OnEnter="Unset" DelayTimer="1.0"/>
				</SetMannequinTag>
				<SetMannequinTag ResetWhenReinstalled="true">
					<Default TagName="Stasis" OnEnter="Unset" DelayTimer="1.0"/>
				</SetMannequinTag>
			</Activities>
		</Action>
