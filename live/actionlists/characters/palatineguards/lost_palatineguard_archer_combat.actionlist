				<TriggerEntity>
					<Default TriggerName="PreProxyOFF"/>
				</TriggerEntity>
				<TriggerEntity>
					<Default TriggerName="SignatureGatingOn"/>
				</TriggerEntity>
				<TriggerEntity>
					<Default TriggerName="OnKneelDeactivate"/>
